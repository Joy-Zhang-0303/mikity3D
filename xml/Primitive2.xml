<mikity>
    <model>
        <group name="sample">
            <box xsize="0.2" ysize="0.2" zsize="0.2" color="red"/>
            <sphere r="0.2" div="30" color="yellow">
                <location x="-0.5" y="0.0" z="0.0"/>
            </sphere>
            <cone r="0.2" height="0.3" div="30" color="lightGray">
                <location x="0.5" y="0.0" z="0.0"/>

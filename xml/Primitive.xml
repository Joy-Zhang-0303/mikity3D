<?xml version="1.0" encoding="UTF-8"?>
<mikity>
    <model>
        <group name="sample">
            <box xsize="0.4" ysize="0.2" zsize="0.2" color="red">
                <rotation xrotate="90.0" yrotate="0.0" zrotate="0.0"/>
            </box>
            <box xsize="0.2" ysize="0.2" zsize="0.2" color="pink">
                <rotation xrotate="90.0" yrotate="0.0" zrotate="0.0"/>
                <location x="0.0" y="0.0" z="-0.6"/>
            </box>
            <box xsize="0.2" ysize="0.2" zsize="0.2" color="blue">
                <location x="0.0" y="0.9" z="0.0"/>
            </box>
            <cylinder r="0.1" height="0.2" div="30" color="orange">
                <rotation xrotate="90.0" yrotate="0.0" zrotate="0.0"/>
                <location x="0.0" y="0.4" z="0.6"/>
            </cylinder>
            <cylinder r="0.1" height="0.2" div="30" color="orange">
                <rotation xrotate="90.0" yrotate="0.0" zrotate="0.0"/>
                <location x="0.0" y="-0.4" z="-0.6"/>
            </cylinder>
            <cylinder r="0.1" height="0.2" div="30" color="orange">
                <rotation xrotate="90.0" yrotate="0.0" zrotate="0.0"/>
                <location x="0.0" y="-0.4" z="0.6"/>
            </cylinder>
        </group>
    </model>
    <config>
        <background color="pink"/>
        <light x="0.2" y="-0.8" z="-0.8"/>
        <view xrotate="-0.2" yrotate="0.0" zrotate="0.0" x="0.0" y="0.3" z="1.0"/>
        <modelUnit angle="radian" length="m"/>
        <dataUnit angle="degree" length="mm"/>
    </config>
</mikity>

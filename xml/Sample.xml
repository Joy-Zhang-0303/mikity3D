<?xml version="1.0" encoding="UTF-8"?>
<mikity>
    <model>
        <group name="group1">
            <cylinder r="6.0" height="0.1" div="50" color="pink"/>
            <group name="group2">
                <box xsize="2.0" ysize="2.0" zsize="2.0" color="yellow">
                    <location x="3.0" y="3.0" z="0.0"/>
                </box>
                <box xsize="1.5" ysize="1.5" zsize="1.5" color="red">
                    <location x="-3.0" y="3.0" z="0.0"/>
                </box>
                <linkdata target="theta" column="3"/>
            </group>
        </group>
    </model>
    <config>
        <data>config/data</data>
        <background color="white"/>
        <light x="0.2" y="-0.8" z="-0.8"/>
        <view xrotate="-0.2" yrotate="0.0" zrotate="0.0" x="0.0" y="0.3" z="1.0"/>
        <modelUnit angle="degree" length="m"/>
        <dataUnit angle="radian" length="m"/>
    </config>
</mikity>
